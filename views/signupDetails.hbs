<div class="signupDetails">
    <div class="leftside">
         <a href="/"><img id="titleImg" src="./img/title.png" /></a>
         <h1>Let us know a little more about you</h1>
    </div>
    <div class="rightside">
        <!-- fix action-->
        <form id="signupDetailsForm" method="POST" action="/register">
            <input type="text" placeholder="imageurl" name="url" id="url">
            <textarea type="text" rows="5" name="details" id="details" placeholder="a few lines about yourself..."></textarea>
            <input type="text" placeholder="country" name="country" id="country"></input>
            <input type="date" name="date"  placeholder="date of birth (mm/dd/yyyy)"></input>
            <button type="submit">complete registration</button>
        </form>
    </div>
</div>