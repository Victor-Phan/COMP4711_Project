<!--The search header bar including the KB logo, search bar, and logout button.-->
<div class="div-search">
    <div class="img-logo-kb">
        <a href="/"><img src="./img/kb.png"></a>
    </div>
    <div class="form-search">
        <form class="searchEntry" id="searchForm" action="/search" method="GET">
            <input name="subject" placeholder="Search" id="searchInputEntry">
            
        </form>
    </div>
    <div class="div-button-logout">
        <form action="/signout" method="POST">
            <button class="button-logout">logout</button>
        </form>
    </div>
</div>